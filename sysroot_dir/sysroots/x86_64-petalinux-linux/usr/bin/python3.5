export OEPYTHON3HOME=$realdir/../../usr TERMINFO_DIRS=$realdir/../../etc/terminfo:/etc/terminfo:/usr/share/terminfo:/usr/share/misc/terminfo:/lib/terminfo PYTHONNOUSERSITE=1
exec -a `dirname $realpath`/python3.5 `dirname $realpath`/python3.5.real "$@"
